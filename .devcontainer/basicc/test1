const months = 4
switch(months)
{
    case 1:
        console.log("january");
        break;
         case 2:
        console.log("febuary");
        break;
         case 3:
        console.log("march");
        break;
         case 4:
        console.log("apirl");
        break;
        default:
            console.log("invalid");
            break;
}
