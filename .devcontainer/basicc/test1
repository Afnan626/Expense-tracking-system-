const balance = 2000
//if(balance >1000) console.log("test passed"),console.log("test not passes");
if(balance <400)
{
    console.log("balance smaller than 400")
}
else if(balance <600)
{
    console.log("balance smaller than 600")
}
else if(balance <800)
{
    console.log("balance smaller than 800")
}
else if(balance <1000)
{
    console.log("balance smaller than 1000")
}
else
{
    console.log("balance is smaaller than 2000")
}
